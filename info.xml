<?xml version="1.0"?>
<extension key="custom-imports" type="module">
  <file>custom_imports</file>
  <name>Custom Importer Applications</name>
  <description>This extension provides custom importer applications.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>akosgarai</author>
    <email>szlem01@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/reflexive-communications/custom-imports</url>
